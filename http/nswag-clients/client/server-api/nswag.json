{
  "runtime": "Net60",
  "documentGenerator": {
    "fromDocument": {
      "url": "swagger.json"
    }
  },
  "codeGenerators": {
    "openApiToCSharpClient": {
      "output": "Swagger.cs",
      "className": "{controller}Client",
      "namespace": "Kenny",
      "generateClientInterfaces": true,
      "useBaseUrl": false,
      "generateBaseUrlProperty": true
    }
  }
}
